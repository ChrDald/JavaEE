<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    
  <servlet>
  	<servlet-name>StudentController</servlet-name>
  	<display-name>StudentController</display-name>
  	<servlet-class>FirstMavenProject.web.controller.StudentController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StudentController</servlet-name>
  	<url-pattern>/displayStudent</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>LoginControllerServ</servlet-name>
  	<display-name>LoginController</display-name>
  	<servlet-class>FirstMavenProject.web.controller.LoginController</servlet-class>
  </servlet>
   
  <servlet-mapping>
  	<servlet-name>LoginControllerServ</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>

</web-app>
